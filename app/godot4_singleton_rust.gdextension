[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../libs/target/debug/libgodot4_singleton_rust.so"
linux.release.x86_64 =   "res://../libs/target/release/libgodot4_singleton_rust.so"
windows.debug.x86_64 =   "res://../libs/target/debug/godot4_singleton_rust.dll"
windows.release.x86_64 = "res://../libs/target/release/godot4_singleton_rust.dll"
macos.debug =            "res://../libs/target/debug/libgodot4_singleton_rust.dylib"
macos.release =          "res://../libs/target/release/libgodot4_singleton_rust.dylib"
macos.debug.arm64 =      "res://../libs/target/debug/libgodot4_singleton_rust.dylib"
macos.release.arm64 =    "res://../libs/target/release/libgodot4_singleton_rust.dylib"